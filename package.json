{
  "name": "sfdx-predeploy-hook-org-env",
  "version": "0.0.0-development",
  "description": "sf predeploy hook to export target org details as environment variables",
  "keywords": [
    "sfdx",
    "sfdx-plugin",
    "sf-plugin",
    "sfdx-hook",
    "hook"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amtrack/sfdx-predeploy-hook-org-env.git"
  },
  "license": "MIT",
  "files": [
    "/src",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "test:e2e": "bash scripts/test-e2e.sh",
    "prepublishOnly": "npx --yes oclif manifest"
  },
  "oclif": {
    "bin": "sf",
    "hooks": {
      "prerun": "./src/hook.mjs"
    }
  },
  "dependencies": {
    "@salesforce/sf-plugins-core": "12.2.6"
  }
}
